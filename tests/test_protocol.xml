<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<root>
    <protocols namespace="test">
        <protocol name="utils" id="0xF0">
            <functions>
                <function name="print">
                    <request>
                        <param name="message" type="string" />
                    </request>
                    <response>
                        <param name="status" type="int" />
                    </response>
                </function>
            </functions>

            <events>
                <event name="error">
                    <param name="message_id" type="uint32_t" />
                    <param name="error_code" type="int" />
                </event>
            </events>
        </protocol>
    </protocols>
</root>
